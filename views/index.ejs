<%- include('./partials/headHeader.ejs') %>

<body>
    <div class="listAll">
        <% allItems.forEach((item)=> { %>
            <a href="http://localhost:3000/showItem/<%= item.name %>"><div class="block" id = "<% item.name %>">
                <h4><%= item.name %></h4>
                <p>🥳 Birthday: <%= item.birthday %></p>
                <p>📈 Height: <%= item.height %></p>
                <p>🧐 Stats:</p>
                <ul>
                    <li>🩺 HP: <%= item.stats.hp %></li>
                    <li>🗡️ Attack: <%= item.stats.attack %></li>
                    <li>🛡️ Defense: <%= item.stats.defense %></li>
                </ul>
            </div>
            </a>
        <% }) %>    
    </div>
</body>

